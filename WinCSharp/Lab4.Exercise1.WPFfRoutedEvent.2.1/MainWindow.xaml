<Window TextBoxBase.TextChanged="Window_TextChanged_1" x:Class="Lab4Exercise1.WPFfRoutedEvent._2._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab4Exercise1.WPFfRoutedEvent._2._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid TextBoxBase.TextChanged="Grid_TextChanged_1">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="85,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <RadioButton x:Name="radBut1" Content="Обработка события в Textbox" HorizontalAlignment="Left" Margin="85,104,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radBut2" Content="Обработка события в Grid" HorizontalAlignment="Left" Margin="85,124,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radBut3" Content="Обработка события в Window" HorizontalAlignment="Left" Margin="85,144,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
